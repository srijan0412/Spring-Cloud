<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="alien" class="com.srijan.SpringDemo.Alien" scope="prototype" autowire="byName"> <!-- we can write autowire="byType as well-->
        <property name="age" value="12">
        </property>
    </bean>

    <bean id="com" class="com.anand.SpringDemo.Laptop" primary="true">
    </bean>
    
    <bean id="desktop" class="com.anand.SpringDemo.Desktop">
    </bean>
</beans>
